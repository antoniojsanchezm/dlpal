{
  "name": "dlpal",
  "version": "1.1.2",
  "description": "Desktop app made in Electron, React and Tailwind CSS for downloading YouTube long-format videos and shorts.",
  "main": "./out/main/index.js",
  "author": "antoniojsanchezm",
  "homepage": "https://github.com/antoniojsanchezm/dlpal",
  "scripts": {
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "node clear_dist && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "checksums": "node generate_checksums.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoniojsanchezm/dlpal.git"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.10",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/montserrat": "^5.1.1",
    "@fontsource/roboto": "^5.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/material": "^6.4.2",
    "@tailwindcss/vite": "^4.0.3",
    "autoprefixer": "^10.4.20",
    "checksum": "^1.0.0",
    "electron-updater": "^6.6.2",
    "ffmpeg-on-progress": "^1.0.0",
    "ffmpeg-static-electron": "^2.0.3",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.0",
    "get-video-id": "^4.1.7",
    "got": "^14.4.7",
    "http": "^0.0.1-security",
    "humanize": "^0.0.9",
    "is-empty": "^1.2.0",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "progress-stream": "^2.0.0",
    "react-toastify": "^11.0.5",
    "stream": "^0.0.3",
    "tailwindcss": "^4.0.3",
    "truncate": "^3.0.0",
    "undici": "^7.3.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^6.3.5"
  }
}
